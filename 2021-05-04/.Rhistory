set.seed(5000)
map_data("world") %>%
filter(region=="Nigeria") %>%
ggplot() +
coord_map(xlim=c(2,15), ylim=c(4,14)) +
geom_polygon(aes(x = long, y = lat, group = group), fill="gray98", color="black") +
geom_point(data=water %>% filter(water_source %in% c("Protected Spring", "Protected Shallow Well") & country_name=="Nigeria"), aes(x = lon_deg, y=lat_deg, color=water_source), alpha=.2, size=1.5) +
geom_point(data=nigeria250k, aes(x = long, y = lat, size=pop.scale), shape=19, alpha=1) +
geom_label_repel(data=nigeria250k %>% filter(pop>=1000000), aes(x = long, y = lat, label = name), force=5, fill="gray20", color="white") +
scale_color_manual(values=c("blue", "orange")) +
theme_void() +
theme(plot.background = element_rect(fill="#008753"),
panel.background = element_rect(fill="#008753"),
panel.border = element_blank(),
text = element_text(size=20, color="white"),
legend.position = "right",
legend.direction = "vertical",
plot.title = element_text(hjust=.5),
plot.caption = element_text(size=10, hjust=.5)) +
labs(title = "Protected Water Sources in Nigeria",
color = "",
size = "",
caption = "#TidyTuesday Visualization by @IanAGutierrez | Data via Water Point Data Exchange")
